{
  "name": "stock-portfolio-backend",
  "version": "0.1.0",
  "description": "Stock Portfolio Tracker Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:import": "node --import tsx src/server.ts",
    "dev:legacy": "tsx --loader tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json}"
  },
  "keywords": [
    "portfolio",
    "stocks",
    "finance",
    "api"
  ],
  "author": "Portfolio Tracker Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "axios": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.2",
    "openai": "^5.10.2",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^30.0.5",
    "prettier": "^3.0.2",
    "prisma": "^6.12.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.3",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  }
}
